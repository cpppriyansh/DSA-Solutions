// Online C++ compiler to run C++ program online
#include <iostream>
#include <string>
using namespace std;

bool rpresent(string p, int i) {
    if (i >= p.size()) {
        return false;
    }

    if (p[i] == 'r') { 
        return true;
    }
    return rpresent(p, i + 1); 
}

int main() {
    string p = "priyansh";
    int i = 0;
    bool ans = rpresent(p, i);
    
    cout<< (ans ? "True" : "False");
    return 0;
}
